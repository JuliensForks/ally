{
  "name": "@adonisjs/ally",
  "version": "2.1.3",
  "description": "Social authentication provider for AdonisJS",
  "main": "build/providers/AllyProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json adonisjs/adonis-ally"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "social-auth",
    "oauth"
  ],
  "author": "adonisjs,virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/assembler": "^3.0.6",
    "@adonisjs/core": "^5.0.4-preview-rc-2.1",
    "@adonisjs/mrm-preset": "^2.4.0",
    "@adonisjs/require-ts": "^1.1.1",
    "@poppinss/dev-utils": "^1.0.11",
    "@types/node": "^14.14.14",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-adonis": "^1.0.15",
    "eslint-plugin-prettier": "^3.3.0",
    "github-label-sync": "^2.0.0",
    "husky": "^4.3.6",
    "japa": "^3.1.1",
    "mrm": "^2.5.13",
    "nock": "^13.0.5",
    "np": "^7.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/utils": "^3.0.3",
    "got": "^11.8.1",
    "luxon": "^1.25.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-ally.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-ally/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-ally#readme",
  "publishConfig": {
    "tag": "alpha",
    "access": "public"
  }
}
